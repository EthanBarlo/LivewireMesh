{
  "name": "livewiremesh-prerender-server",
  "version": "1.0.0",
  "description": "Node.js prerender server for LivewireMesh React components",
  "type": "module",
  "main": "src/server.ts",
  "bin": {
    "livewiremesh-prerender": "./bin/prerender.js"
  },
  "scripts": {
    "start": "tsx src/server.ts",
    "dev": "tsx watch src/server.ts",
    "build": "vite build",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "express": "^4.21.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vite": "^7.3.0"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  }
}
